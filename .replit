run = "python3 main.py"
modules = ["python-3.11"]

[[ports]]
localPort = 3000
externalPort = 80

[[ports]]
localPort = 5000
externalPort = 5000

[[ports]]
localPort = 8000
externalPort = 8000
exposeLocalhost = true

[nix]
channel = "stable-24_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "GET MONEY"
mode = "sequential"
author = 42692233

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "uvicorn main:app --reload"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "NeuroSync"

[[workflows.workflow]]
name = "NeuroSync"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "uvicorn main:app --host 0.0.0.0 --port 5000 --reload"
waitForPort = 5000

[deployment]
run = ["sh", "-c", "python main.py"]
